{
  "inbound": {
    "port": PORT,
    "protocol": "vmess",
    "settings": {
      "clients": [
        {
          "id": "UUID",
          "alterId": 0
        }
      ]
    },
    "streamSettings": {
      "network": "ws",
      "security": "tls",
      "wsSettings": {
        "path": "/ray"
      },
      "tlsSettings": {
        "allowInsecure": true,
        "certificates": [
          {
            "certificateFile": "/srv/DOMAIN_cert.pem",
            "keyFile": "/srv/DOMAIN_key.pem"
          }
        ]
      }
    }
  },
  "outbound": {
    "protocol": "freedom",
    "settings": {}
  }
}
